<view class="container"><view class="header" hidden="{{true}}"><view data-event-opts="{{[['tap',[['switchTab',['all']]]]]}}" class="{{[current==0?'active':'']}}" bindtap="__e">所有记录</view><view data-event-opts="{{[['tap',[['switchTab',['my']]]]]}}" class="{{[current==1?'active':'']}}" bindtap="__e">我的记录</view></view><block wx:if="{{showEmpty}}"><view style="padding-top:200rpx;"><empty vue-id="42dbe0ae-1" title="{{emptyTitle}}" data-event-opts="{{[['^updateTitle',[['__set_sync',['$0','emptyTitle','$event'],['']]]]]}}" bind:updateTitle="__e" bind:__l="__l"></empty></view></block><block wx:if="{{result.length>0}}"><view class="content" style="padding-top:0px;"><block wx:for="{{result}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['result','',index]]]]]]]}}" class="content-list" bindtap="__e"><view class="information"><view class="st-title"><view class="title">{{"车牌号-"+item.num}}</view><view class="description">{{"序号："+item.xh}}</view><view class="description">{{"车辆名称："+item.name}}</view><view class="description">{{"准乘人数："+item.seatNum}}</view><view class="description">{{"下一次年检日期："+item.nextCheckDate}}</view></view></view><view class="status" style="{{('color:'+(item.status==1?'green':'red'))}}">{{''+(item.status==1?'正常':'暂停')+''}}</view></view></view></block><block wx:if="{{bottomLoading}}"><i-load-more vue-id="42dbe0ae-2" bind:__l="__l"></i-load-more></block><block wx:if="{{searchNone}}"><i-load-more vue-id="42dbe0ae-3" tip="没有更多数据" loading="false" bind:__l="__l"></i-load-more></block></view></block><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="scrollBox img-center" style="bottom:8%;right:12%;" bindtap="__e"><image style="width:34rpx;height:34rpx;" src="../../static/images/add.png" mode="aspectFit"></image></view><block wx:if="{{showBackTopBtn}}"><view data-event-opts="{{[['tap',[['backTop',['$event']]]]]}}" class="scrollBox img-center" style="bottom:18%;right:12%;" bindtap="__e"><image style="width:34rpx;height:34rpx;" src="../../static/images/stick.png" mode="aspectFit"></image></view></block></view>