<view class="container"><view class="header"><view data-event-opts="{{[['tap',[['switchTab',['all']]]]]}}" class="{{[current=='all'?'active':'']}}" bindtap="__e">所有公告</view><view data-event-opts="{{[['tap',[['switchTab',['my']]]]]}}" class="{{[current=='my'?'active':'']}}" bindtap="__e">我的公告</view></view><block wx:if="{{showEmpty}}"><view style="padding-top:200rpx;"><empty vue-id="8fbf6bc2-1" title="{{emptyTitle}}" data-event-opts="{{[['^updateTitle',[['__set_sync',['$0','emptyTitle','$event'],['']]]]]}}" bind:updateTitle="__e" bind:__l="__l"></empty></view></block><block wx:if="{{tweetList.length>0}}"><view class="content" style="padding-top:85rpx;"><block wx:for="{{tweetList}}" wx:for-item="tweet" wx:for-index="index" wx:key="index"><view class="item"><view data-event-opts="{{[['tap',[['goDetail',[index,'$0'],[[['tweetList','',index]]]]]]]}}" class="row" bindtap="__e"><view class="user-info"><view class="icon"><image src="{{tweet.user.avatar}}"></image></view><view class="title">{{tweet.user.name}}</view></view><view class="information"><view class="st-title" style="justify-content:flex-start;"><view style="{{('display:'+(tweet.isRead==0?'none':'flex')+';font-size: 20rpx;color: #428bca;padding-right: 30rpx;')}}">●</view><view class="title" style="{{('color:#'+(tweet.isRead==0?'999':'212121'))}}">{{tweet.content}}</view></view></view><view class="information"><view class="st-bottom"><view class="description"><i-icon vue-id="{{'8fbf6bc2-2-'+index}}" type="time" size="18" color="#80848f" bind:__l="__l"></i-icon><view style="margin-top:3rpx;">{{tweet.pubDate}}</view></view><view class="description" style="display:none;"><i-icon vue-id="{{'8fbf6bc2-3-'+index}}" type="message" size="18" color="#80848f" bind:__l="__l"></i-icon><view style="margin-top:3rpx;">{{tweet.commentCount}}</view></view></view></view></view></view></block><block wx:if="{{bottomLoading}}"><i-load-more vue-id="8fbf6bc2-4" bind:__l="__l"></i-load-more></block><block wx:if="{{searchNone}}"><i-load-more vue-id="8fbf6bc2-5" tip="没有更多数据" loading="false" bind:__l="__l"></i-load-more></block></view></block><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="scrollBox img-center" style="{{('bottom: 8%;right:12%;'+(userRole>4?'display:none':''))}}" bindtap="__e"><image style="width:34rpx;height:34rpx;" src="../../static/images/add.png" mode="aspectFit"></image></view><block wx:if="{{showBackTopBtn}}"><view data-event-opts="{{[['tap',[['backTop',['$event']]]]]}}" class="scrollBox img-center" style="bottom:18%;right:12%;" bindtap="__e"><image style="width:34rpx;height:34rpx;" src="../../static/images/stick.png" mode="aspectFit"></image></view></block></view>