<view class="container"><view class="header"><view data-event-opts="{{[['tap',[['switchTab',['all']]]]]}}" class="{{[current==0?'active':'']}}" bindtap="__e">所有预约</view><view data-event-opts="{{[['tap',[['switchTab',['my']]]]]}}" class="{{[current==1?'active':'']}}" bindtap="__e">我的预约</view></view><block wx:if="{{showEmpty}}"><view style="padding-top:200rpx;"><empty vue-id="137be07b-1" title="{{emptyTitle}}" data-event-opts="{{[['^updateTitle',[['__set_sync',['$0','emptyTitle','$event'],['']]]]]}}" bind:updateTitle="__e" bind:__l="__l"></empty></view></block><block wx:if="{{result.length>0}}"><view class="content"><block wx:for="{{result}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['result','',index]]]]]]]}}" class="content-list" bindtap="__e"><view class="information"><view class="icon"><image src="{{item.user.avatar}}"></image></view><view class="st-title"><view class="title">{{"目的地-"+item.destination}}</view><view class="description">{{"申请事由："+item.resion}}</view><view class="time">{{"乘车人数："+item.members+"人"}}</view><view class="time"><text space="ensp">乘 车 人：</text>{{item.memberNames}}</view><view class="time">{{"开始时间："+item.startTime}}</view><view class="time">{{"结束时间："+item.endTime}}</view><view class="description"><text space="ensp">申 请 人：</text>{{item.deptName+"-"+item.user.name}}</view></view></view><view class="{{['status '+item.status]}}" hidden="{{item.carNum==undefined}}"><view class="car-num">{{item.carNum}}</view></view><view class="{{['status '+item.status]}}" hidden="{{item.carNum!=undefined}}">{{''+(item.status=='NEW'?'待审批':item.status=='AGREE'?'通过':'驳回')+''}}</view></view></view></block><block wx:if="{{bottomLoading}}"><i-load-more vue-id="137be07b-2" bind:__l="__l"></i-load-more></block><block wx:if="{{searchNone}}"><i-load-more vue-id="137be07b-3" tip="没有更多数据" loading="false" bind:__l="__l"></i-load-more></block></view></block><view data-event-opts="{{[['tap',[['add',['$event']]]]]}}" class="scrollBox img-center" style="bottom:8%;right:12%;" bindtap="__e"><image style="width:34rpx;height:34rpx;" src="../../static/images/add.png" mode="aspectFit"></image></view><block wx:if="{{showBackTopBtn}}"><view data-event-opts="{{[['tap',[['backTop',['$event']]]]]}}" class="scrollBox img-center" style="bottom:18%;right:12%;" bindtap="__e"><image style="width:34rpx;height:34rpx;" src="../../static/images/stick.png" mode="aspectFit"></image></view></block></view>